@model HelloASP.ViewModels.HomeIndexViewModel
<!DOCTYPE html>

<html>
<head>
    <title>title</title>
</head>
<body>
<div>
    <h1>@Model.CurrentMessage</h1>
    <table>
        @foreach (var restaurant in Model.Restaurants)
        {
            <tr>
                <td>@restaurant.Id</td>
                <td>@restaurant.Name</td>
                <td><a asp-action="Details" asp-route-id="@restaurant.Id">Details</a></td>
            </tr>
        }
    </table>
    <a asp-action="Create">Add Restaurant</a>
</div>
</body>
</html>